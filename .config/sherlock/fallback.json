    {
        "name": "Bookmarks",
        "alias": "",
        "type": "command",
        "args": {"commands": {
            "ChatGPT": {"icon": "chat-gpt", "exec": "xdg-open https://chatgpt.com &", "search_string": "chatgpt;ai;"},
